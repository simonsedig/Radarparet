@model List<Grup__8_Annonser.ServiceReferenceAnnonser.AnnonsKlass>
@{
    ViewBag.Title = "Read";
}

<h1>Read annons</h1>
<hr />
@Html.ActionLink("Skapa en ny reklam", "Create")
<br />
<br />
<table>
    <tr>
        <th>Annons: </th>

        <th>Hoovertext: </th>

    </tr>
    @*Skriver ut all data från databasen genom en "foreach"*@
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.resource</td>
            <td>@item.onHooverText</td>
            <td>@Html.ActionLink("Radera", "Delete", new { id = item.addId })</td>
            <td>@Html.ActionLink("Uppdatera", "Update", new { id = item.addId })</td>
        </tr>
    }
</table>


